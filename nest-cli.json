{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/video-cms/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/video-cms/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/video-cms",
  "projects": {
    "cloudinary": {
      "type": "library",
      "root": "libs/cloudinary",
      "entryFile": "index",
      "sourceRoot": "libs/cloudinary/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cloudinary/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "databases": {
      "type": "library",
      "root": "libs/databases",
      "entryFile": "index",
      "sourceRoot": "libs/databases/src",
      "compilerOptions": {
        "tsConfigPath": "libs/databases/tsconfig.lib.json"
      }
    },
    "discovery": {
      "type": "application",
      "root": "apps/discovery",
      "entryFile": "main",
      "sourceRoot": "apps/discovery/src",
      "compilerOptions": {
        "tsConfigPath": "apps/discovery/tsconfig.app.json"
      }
    },
    "video-cms": {
      "type": "application",
      "root": "apps/video-cms",
      "entryFile": "main",
      "sourceRoot": "apps/video-cms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/video-cms/tsconfig.app.json"
      }
    }
  }
}